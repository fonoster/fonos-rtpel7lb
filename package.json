{
  "name": "fonos-rtpel7lb",
  "version": "0.0.1",
  "description": "A layer 7 load balancer for RTPEngine.",
  "private": true,
  "nodemonConfig": {
    "watch": [
      "./src/"
    ],
    "ext": "ts",
    "exec": "ts-node",
    "ignore": "**/*.d.ts"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=dev nodemon rtpel7lb.ts",
    "build": "tsc -b tsconfig.json",
    "test": "cross-env NODE_ENV=dev nyc --reporter=lcov mocha -r ts-node/register test/src/consul/consul.test.ts --exit",
    "integration": "cross-env NODE_ENV=dev nyc --reporter=lcov mocha -r ts-node/register test/int.test.ts -t 360000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/fonos-rtpel7lb.git"
  },
  "keywords": [
    "RTPEngine",
    "RTP",
    "VoIP",
    "SIP",
    "Asterisk",
    "Kamailio",
    "Routr"
  ],
  "bin": {
    "run": "rtpel7lb.js",
    "healthcheck": "healthcheck.js"
  },
  "author": "Fonoster Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fonoster/fonos-rtpel7lb/issues"
  },
  "homepage": "https://github.com/fonoster/fonos-rtpel7lb#readme",
  "dependencies": {
    "@types/bencode": "^2.0.0",
    "@types/ioredis": "^4.19.3",
    "bencode": "^2.0.1",
    "bluebird": "^3.7.2",
    "chai-as-promised": "^7.1.1",
    "consul": "^0.39.0",
    "ioredis": "^4.19.4",
    "rtpengine-client": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/consul": "^0.23.34",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/sinon": "^9.0.10",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.2.1",
    "nanoid": "^3.1.12",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier-standard"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty && node etc/gen_docs.js && git add docs"
    }
  }
}
